#foreach ($person in $persons)
  <a href="/persons/$person.getId()">Find the $person.getName() animal</a>
  <br />
  <ul>
    <li>Phone: $person.getPhone()</li>
    <li>Animal Type: $person.getAnimalType()</li>
    <li>Breed Type: $person.getBreedType()</li>
  </ul>
#end
<br />
<a href="/persons/new">Add a new Person</a>
